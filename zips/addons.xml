<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.Bigfoot" name="Bigfoot" version="0.0.1" provider-name="Bigfoot">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Hide and Seek World Champion</summary>
		<description lang="en">Full add-on description.</description>
		<platform>all</platform>
		<forum>http://www.noobsandnerds.com/support</forum>
		<website>www.noobsandnerds.com</website>
		<news></news>
		<genre>movies,tvshows,usa</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

<addon id="repository.bigfoot" name="Bigfoot Repo" version="0.0.1" provider-name="Bigfoot">
	<extension point="xbmc.addon.repository" name="Bigfoot Repo">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/oswalt0/repository.bigfoot/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/oswalt0/repository.bigfoot/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/oswalt0/repository.bigfoot/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">The offical repo for Bigfoot</summary>
		<description lang="en">Full repository description.</description>
		<genre>movies,tvshows,usa</genre>
	</extension>
</addon>
</addons>
